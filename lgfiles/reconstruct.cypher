MATCH (n:Entity {file_source: 'logo_classes'}) - [:NAMEVARIANT|ADDRESS] - (nv) WHERE (nv.rowIndex <> '-1') WITH nv, n MATCH (n) - [:NAMEVARIANT] - (c:Caption) WITH nv, n, c.Company as caption RETURN nv.rowIndex as lineno, coalesce(collect(nv.Class)[0],'') as Class, coalesce(collect(nv.Company)[0],caption) as Company, coalesce(collect(nv.CompanyParent)[0],'') as CompanyParent, coalesce(collect(nv.ExampleImage)[0],'') as ExampleImage, coalesce(collect(nv.Visual)[0],'') as Visual, coalesce(collect(nv.RecommendedThreshold)[0],'') as RecommendedThreshold, coalesce(collect(nv.Index)[0],'') as Index, coalesce(collect(nv.VolumePerDay)[0],'') as VolumePerDay, coalesce(collect(nv.AmendedThreshold)[0],'') as AmendedThreshold;