USING PERIODIC COMMIT 500 LOAD CSV WITH HEADERS FROM $param + 'P0 - Military Aircrafts-base.csv' AS line WHERE line.global_identifier IS NOT NULL 
     MERGE (n:Entity {global_identifier: line.global_identifier}) 
     ON CREATE SET n += {"Name": line.name, "Label": line.label, "Types": line.types, "Name_variants": line.name_variants, "Merge_global_identifier": line.merge_global_identifier, "Caption": line.caption, "Wikipedia_url": line.wikipedia_url, "Wikidata_id": line.wikidata_id, "Dbpedia_id": line.dbpedia_id, "Data_source": line.data_source, "Source_identifier": line.source_identifier, "Global_identifier": line.global_identifier, "Description": line.description, "Key_phrases": line.key_phrases, "Handles": line.handles, "Websites": line.websites, "Hashtags": line.hashtags, "Emails": line.emails, "Name": line.Name, "HighLevelCategory": line.`High level category`, "Sub_categories": line.`Sub-categories`, "ManufacturerCountry": line.`Manufacturer Country`, "Quantity": line.Quantity, "Variant": line.Variant, "OperatingCountry": line.`Operating Country`, "OperatingOrganization": line.`Operating Organization`, "Logo": line.logo, "Image": line.image}, n.sources = [], n.source_ids = [] 
     ON MATCH SET n += {"Name": line.name, "Label": line.label, "Types": line.types, "Name_variants": line.name_variants, "Caption": line.caption, "Wikipedia_url": line.wikipedia_url, "Wikidata_id": line.wikidata_id, "Dbpedia_id": line.dbpedia_id, "Description": line.description, "Key_phrases": line.key_phrases, "Handles": line.handles, "Websites": line.websites, "Hashtags": line.hashtags, "Emails": line.emails, "Name": line.Name, "HighLevelCategory": line.`High level category`, "Sub_categories": line.`Sub-categories`, "ManufacturerCountry": line.`Manufacturer Country`, "Quantity": line.Quantity, "Variant": line.Variant, "OperatingCountry": line.`Operating Country`, "OperatingOrganization": line.`Operating Organization`, "Logo": line.logo, "Image": line.image}, n.sources = apoc.coll.toSet(n.sources + line.data_source), n.source_ids = apoc.coll.toSet(n.source_ids + line.source_identifier) 
     RETURN n;