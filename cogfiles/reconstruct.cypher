MATCH (n:Entity {file_source: 'EntityCollectionTask_Cognizant'}) - [:NAMEVARIANT|ADDRESS] - (nv) WHERE (nv.rowIndex <> '-1') WITH nv, n MATCH (n) - [:NAMEVARIANT] - (c:Caption) WITH nv, n, c.COMPANY_NAME as caption RETURN nv.rowIndex as lineno, coalesce(collect(nv.Assignee)[0],'') as Assignee, coalesce(collect(nv.COMPANY_NAME)[0],caption) as COMPANY_NAME, coalesce(collect(nv.URL)[0],'') as URL, coalesce(collect(nv.HQ_ADDRESS)[0],'') as HQ_ADDRESS, coalesce(collect(nv.Lat)[0],'') as Lat, coalesce(collect(nv.Lon)[0],'') as Lon, coalesce(collect(nv.`Complete?`)[0],'') as `Complete?`;